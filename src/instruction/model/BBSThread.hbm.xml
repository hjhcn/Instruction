<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="instruction.model.BBSThread" table="bbs_thread" catalog="instruction">
        <id name="id">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="100" not-null="true" />
        </property>
        <property name="link" type="java.lang.String">
            <column name="link" length="255" not-null="true" />
        </property>
        <property name="order" type="java.lang.Integer">
            <column name="_order" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
